{
    "name": "kamrul/press",
    "description": "An elegant markdown-power blocg for the Laravel",
    "license": "MIT",
    "authors": [
        {
            "name": "Kamrul Islam",
            "email": "anik825@gmail.com"
        }
    ],
    "require-dev": {
        "orchestra/testbench": "^6.20",
        "phpunit/phpunit": "^9",
        "mockery/mockery": "^1.4"
    },
    "autoload": {
        "psr-4": {
            "kamrul\\Press\\": "src/"
        },
        "classmap": [
            "database/factories"
        ]
    },
    "autoload-dev": {
	"psr-4": {
	    "kamrul\\Press\\Tests\\": "tests/"
    }
    },
    "require": {
        "erusev/parsedown": "^1.7",
        "laravel/legacy-factories": "^1.1"
    },
    "extra": {
        "laravel": {
            "providers": [
                "kamrul\\Press\\PressBaseServiceProvider"
            ]
        }
    }
}
